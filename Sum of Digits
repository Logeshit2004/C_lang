#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() { 
    int n, sum = 0;
    scanf("%d", &n);
    if (n >= 10 && n <= 99) {
        int digit1 = n / 10;     
        int digit2 = n % 10;     
        sum = digit1 + digit2;
        printf("Sum of Digit %02d is %03d\n", n, sum); 
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}

