#include <stdio.h>

int main() {
    float speed, distance, time;
    scanf("%f", &speed);
    scanf("%f", &distance);
    scanf("%f", &time);

    float speed_per_sec = speed / 3600.0;
    float time_to_reach = distance / speed_per_sec;

    if (time_to_reach < time) {
        printf("Yes\n");
    } else {
        printf("No\n");
    }

    return 0;
}
