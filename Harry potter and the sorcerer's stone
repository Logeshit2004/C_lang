#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter 
    your code here. Read input from STDIN. Print output to STDOUT */
    int num, first, last, result;
    scanf("%d", &num); 
    last = num % 10;
    first = num;
    while (first >= 10) {
        first /= 10;
    }
    result = first + last;
    printf("%d\n", result);
    return 0;
}
